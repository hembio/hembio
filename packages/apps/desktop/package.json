{
  "name": "@hembio/desktop-app",
  "productName": "hembio",
  "version": "0.1.0",
  "description": "Hembio Desktop Application",
  "main": ".webpack/main",
  "scripts": {
    "dev": "cross-env ELECTRON_ENV=development electron-forge start",
    "package": "cross-env ELECTRON_ENV=production electron-forge package",
    "make": "cross-env ELECTRON_ENV=production electron-forge make",
    "publish": "cross-env ELECTRON_ENV=production electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "Marcus Ekwall",
    "email": "marcus.ekwall@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "desktop2"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window"
                }
              ]
            },
            "port": 3010,
            "loggerPort": 9010
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.58",
    "@electron-forge/maker-deb": "^6.0.0-beta.58",
    "@electron-forge/maker-rpm": "^6.0.0-beta.58",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.58",
    "@electron-forge/maker-zip": "^6.0.0-beta.58",
    "@electron-forge/plugin-webpack": "6.0.0-beta.58",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/eslint": "^7.28.0",
    "@types/prop-types": "^15.7.4",
    "@types/react": "^17.0.14",
    "@types/webpack-node-externals": "^2.5.2",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "electron": "^13.1.7",
    "eslint": "^7.31.0",
    "eslint-plugin-import": "^2.23.4",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.13",
    "node-loader": "^2.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  },
  "installConfig": {
    "hoistingLimits": "dependencies"
  }
}
